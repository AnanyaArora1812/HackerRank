from collections import defaultdict

def count_subsequences(nums, k):
    prefix = 0
    remainder_count = defaultdict(int)
    remainder_count[0] = 1   # empty prefix
    result = 0
    
    for num in nums:
        prefix += num
        r = prefix % k
        result += remainder_count[r]   # subsequences ending here
        remainder_count[r] += 1
    
    return result

if __name__ == "__main__":
    T = int(input())
    for _ in range(T):
        n, k = map(int, input().split())
        nums = list(map(int, input().split()))
        print(count_subsequences(nums, k))
